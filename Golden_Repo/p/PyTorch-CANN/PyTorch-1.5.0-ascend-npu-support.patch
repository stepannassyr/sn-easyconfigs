+
+Tensor& fill_diagonal_out_npu(
+    Tensor& result,
+    const Tensor& self,
+    Scalar& value,
+    bool wrap) {
+  float fill_value = CalcuOpUtil::get_scalar_float_value(value);
+  OpCommand cmd;
+  cmd.Name("FillDiagonal")
+      .Input(self)
+      .Output(result)
+      .Attr("fill_value", fill_value)
+      .Attr("wrap", wrap)
+      .Run();
+
+  return result;
