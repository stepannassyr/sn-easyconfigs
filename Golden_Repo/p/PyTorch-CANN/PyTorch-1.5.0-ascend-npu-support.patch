+    Tensor otherBroadcast = other.expand(self.sizes());
+    OpCommand cmd;
+    cmd.Name("LeftShift")
+       .Input(self)
+       .Input(otherBroadcast)
+       .Output(result)
+       .Run(); 
+
+  return result;
