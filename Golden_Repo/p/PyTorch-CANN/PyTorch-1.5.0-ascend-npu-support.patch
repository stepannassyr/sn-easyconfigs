+
+  return std::make_tuple(
+      std::move((std::get<0>(output)).squeeze(2).permute({2, 0, 1})), 
+      std::move((std::get<1>(output)).squeeze(2).permute({2, 1, 0})), 
+      std::move(std::get<2>(output)));
