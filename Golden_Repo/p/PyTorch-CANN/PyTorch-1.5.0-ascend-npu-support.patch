+      roi_align_npu_output_size(self, rois, pooled_height, pooled_width);
+
+  // construct the output tensor of the NPU
+  Tensor result =
+      OpPreparation::ApplyTensorWithFormat(self, outputSize, ACL_FORMAT_NC1HWC0);
