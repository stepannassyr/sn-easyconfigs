+Tensor& ceil_out_npu(Tensor& result, const Tensor& self) {
+  // executing the NPU operator
+  OpCommand cmd;
+  cmd.Name("Ceil")
+      .Input(self)
+      .Output(result)
+      .Run();
+
+  return result;
+}
+
+Tensor ceil_npu(const Tensor& self) {
+  Tensor result = OpPreparation::ApplyTensor(self);
+  ceil_out_npu(result, self);
+  return result;
