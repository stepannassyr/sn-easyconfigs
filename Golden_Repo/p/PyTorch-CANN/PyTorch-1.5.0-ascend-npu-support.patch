+bitset<64> make_dim_mask(IntArrayRef dims, int64_t ndim) {
+  bitset<64> mask = bitset<64>();
+  if (dims.empty()) {
+    mask.flip();
+  } else {
+    for (int64_t dim : dims) {
+      mask.set(make_wrap_dim(dim, ndim));
+    }
+  }
+
+  return mask;
+}
