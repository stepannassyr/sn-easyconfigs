+  SmallVector<int64_t, N> sizeVec = array_to_small_vector(result.sizes());
+  OpCommand cmd;
+  cmd.Name("Conv2DTranspose")
+      .Input(sizeVec, at::kInt)
+      .Input(input, "x", ACL_FORMAT_NCHW)
+      .Input(weight, "filter", ACL_FORMAT_NCHW);
+  if (bias.defined()) {
