+float CalcuOpUtil::get_scalar_float_value(const Scalar& scalar) {
+  float value;
+  if (scalar.isFloatingPoint()) {
+    value = scalar.toFloat();
+  } else {
+    value = (float)scalar.toInt();
+  }
+
+  return value;
+}
