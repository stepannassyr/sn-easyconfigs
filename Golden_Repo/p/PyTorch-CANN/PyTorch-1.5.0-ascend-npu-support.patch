+
+Tensor& pad_out_npu(
+    Tensor& output,
+    const Tensor& input,
+    IntArrayRef paddings) {
+  SmallVector<int64_t, N> paddingsVector = array_to_small_vector(paddings);
+  paddingsVector.resize(2 * input.dim(), 0);
+
+  OpCommand cmd;
+  cmd.Name("Pad")
