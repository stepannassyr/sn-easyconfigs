+    __and___out_npu(result, other, self.item());
+  } else {
+    OpCommand cmd;
+    cmd.Name((self.scalar_type() == ScalarType::Bool) ? "LogicalAnd" : "BitwiseAnd")
+       .Input(self)
+       .Input(other)
+       .Output(result)
+       .Run(); 
+  }
+
+  return result;
