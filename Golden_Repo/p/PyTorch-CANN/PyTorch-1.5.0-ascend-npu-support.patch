+
+tuple<Tensor, Tensor, Tensor> conv_tbc_backward_npu(
+    const Tensor& self,
+    const Tensor& input,
+    const Tensor& weight,
+    const Tensor& bias,
+    int64_t pad) {
+  // construct the output tensor of the NPU
+  auto output = npu_conv2d_backward(
