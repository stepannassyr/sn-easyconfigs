+    if (status != c10::npu::NpuSysCtrl::SysStatus::INIT_SUCC) {
+        NPU_LOGE("Npu init fail.");
+    }
+  }
+
+  int pre_device = 0;
+  auto ret = aclrtGetDevice(&pre_device);
+  if (ret != ACL_ERROR_NONE){
+      C10_NPU_CHECK(aclrtSetDevice(device));
+  } else if (pre_device != device) {
