+
+Tensor abs_npu(const Tensor& self) {
+  OpPipeWithApplyOut pipe;
+  return pipe.ApplyOutputSameAs(self)
+    .Func([&self](Tensor& result) {abs_out_npu_nocheck(result, self);})
+    .Call();
+}
+
+Tensor& abs_npu_(Tensor& self) {
+  abs_out_npu(self, self);
+  return self;
+}
