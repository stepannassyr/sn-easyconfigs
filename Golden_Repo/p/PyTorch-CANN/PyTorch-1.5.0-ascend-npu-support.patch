+  Tensor weight_cpu = weight.to("cpu").requires_grad_();
+  Tensor indices_cpu = indices.to("cpu");
+  Tensor offsets_cpu = offsets.to("cpu");
+  Tensor per_sample_weights_cpu = per_sample_weights;
+  if (per_sample_weights_cpu.defined()) {
