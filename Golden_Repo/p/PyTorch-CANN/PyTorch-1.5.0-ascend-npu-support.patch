+
+tuple<Tensor&, Tensor&, Tensor&> dropout_v2_out_npu(
+    Tensor& result,
+    Tensor& mask,
+    Tensor& new_seed,
+    const Tensor& self,
+    Tensor& seed,
+    double p) {
+
+  OpCommand cmd;
+  cmd.Name("DropoutV2")
+      .Input(self)
+      .Input(seed)
+      .Output(result)
+      .Output(mask)
+      .Output(new_seed)
+      .Attr("p", static_cast<float>(p))
+      .Run();
+  
+  return tuple<Tensor&, Tensor&, Tensor&>(result, mask, new_seed);
+}
