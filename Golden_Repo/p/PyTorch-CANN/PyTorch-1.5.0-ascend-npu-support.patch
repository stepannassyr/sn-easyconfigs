+std::tuple<Tensor, Tensor> _symeig_helper_npu(const Tensor& self, bool eigenvectors, bool upper) {
+  auto self_sizes = self.sizes().vec();
+  self_sizes.pop_back();
+  auto eigvals = at::empty(self_sizes, self.options());
+
+  if (self.numel() == 0) {
