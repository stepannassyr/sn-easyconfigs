+SmallVector<int64_t, SIZE> repeat_interleave_npu_output_size(
+    const Tensor& self,
+    int64_t repeats,
+    int64_t dim) {
+  SmallVector<int64_t, SIZE> shape;
+  if (dim < 0) {
+	  dim = dim + self.dim();
+  }
+  for (int64_t i = 0; i < self.dim(); i++) {
+    if (i == dim) {
+      shape.emplace_back(self.size(i) * repeats);
+    } else {
+      shape.emplace_back(self.size(i));
+    }
+  }
+  return shape;
+}
