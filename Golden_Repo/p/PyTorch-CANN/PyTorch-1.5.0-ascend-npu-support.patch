+Tensor& sort_without_indices_out_npu(
+    Tensor& result,
+    const Tensor& self,
+    int64_t dim,
+    bool descending) {
+  dim = CalcuOpUtil::make_wrap_dim(dim, self.dim());
+  int64_t lastDim = CalcuOpUtil::make_wrap_dim(-1, self.dim());
+
+  if (dim != lastDim) {
+    SmallVector<int64_t, SHAPE_SIZE> perm;
+    for (int64_t i = 0; i < self.dim(); i++) {
