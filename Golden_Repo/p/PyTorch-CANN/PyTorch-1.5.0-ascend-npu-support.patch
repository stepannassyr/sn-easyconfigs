+Tensor& abs_out_npu(Tensor& result, const Tensor& self) {
+  OpPipeWithDefinedOut pipe;
+  return pipe.CheckMemory({self}, {result})
+   .Func([&self](Tensor& result){abs_out_npu_nocheck(result, self);})
+   .Call(result);
