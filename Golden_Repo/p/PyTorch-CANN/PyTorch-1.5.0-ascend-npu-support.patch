+bool ReleaseQueue::IsEmptyQueue() const {
+  return read_idx.idx == write_idx.idx;
+}
+
+bool ReleaseQueue::IsFullQueue() const {
+  return ((write_idx.idx + 1) % kReleaseQueueCapacity) == read_idx.idx;
+}
