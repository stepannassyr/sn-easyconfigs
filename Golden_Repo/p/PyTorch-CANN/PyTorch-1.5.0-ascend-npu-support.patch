+OpPipeWithApplyOut& OpPipeWithApplyOut::ApplyOutputSameAs(const Tensor& src) {
+  this->dst = OpPreparation::ApplyTensor(src);
+  return *this;
+}
+
+Tensor& OpPipeWithApplyOut::Call() {
+  this->func(this->dst);
+  return this->dst;
+}
