+static const std::unordered_map<c10::ScalarType, int, ScalarTypeHashFunction> scalarTypeToDumpTypeMap = {
+  {c10::kFloat, 1},
+  {c10::kByte, 2},
+  {c10::kChar, 3},
+  {c10::kShort, 5},
+  {c10::kInt, 6},
+  {c10::kLong, 7},
+  {c10::kBool, 9},
+  {c10::kHalf, 10},
+  {c10::kDouble, 11},
+};
