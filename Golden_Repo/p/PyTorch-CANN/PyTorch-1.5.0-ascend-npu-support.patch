+typedef aclError (*aclopCompileFunc)(
+    int numInputs,
+    const aclTensorDesc* const inputDesc[],
+    int numOutputs,
+    const aclTensorDesc* const outputDesc[],
+    const aclopAttr* opAttr,
+    aclopKernelDesc* aclopKernelDesc);
+
+/**
+ * @ingroup AscendCL
+ * @brief register compile function
