+  C10_NPU_CHECK(aclrtSynchronizeStream(stream));
+
+  C10_NPU_CHECK(aclmdlInitDump());
+  const char *aclConfigPath = "acl.json";
+  C10_NPU_CHECK(aclmdlSetDump(aclConfigPath));
+
+  C10_NPU_CHECK(aclrtSynchronizeStream(stream));
+#endif
