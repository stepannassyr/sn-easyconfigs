+  if (!NpuUtils::check_match(&self)) {
+    Tensor contiguousSelf = NpuUtils::format_contiguous(self);
+    pow_out_npu(contiguousSelf, contiguousSelf, exp);
+    NpuUtils::format_fresh_view(self, contiguousSelf);
+  } else {
+    pow_out_npu(self, self, exp);
+  }
+
+  return self;
+}
