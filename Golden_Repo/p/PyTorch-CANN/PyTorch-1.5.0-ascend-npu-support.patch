+static int64_t ScalarTypeToDumpType(const c10::ScalarType& st) {
+  int64_t dump_type = -1;
+  const auto it = scalarTypeToDumpTypeMap.find(st);
+  if (it != scalarTypeToDumpTypeMap.end()) {
+    dump_type = it->second;
+  }
+  return dump_type;
+}
