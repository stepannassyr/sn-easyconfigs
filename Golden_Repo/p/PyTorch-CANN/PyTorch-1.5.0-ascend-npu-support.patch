+ *
+ * @retval ACL_ERROR_NONE The function is successfully executed.
+ * @retval OtherValues Failure
+*/
+ACL_FUNC_VISIBILITY aclError aclmdlFinalizeDump();
+
+#ifdef __cplusplus
+}
+#endif
