+
+Tensor linspace_assist(int64_t steps) {
+  SmallVector<float, N> assist;
+  assist.resize(steps);
+
+  for (int64_t i = 0; i < steps; i++) {
+    assist[i] = (float)(i);
+  }
+  Tensor assistTensor =
+      from_blob(assist.data(), {steps}, dtype(ScalarType::Float));
+  return CalcuOpUtil::copy_tensor_host_to_device(assistTensor);
+}
