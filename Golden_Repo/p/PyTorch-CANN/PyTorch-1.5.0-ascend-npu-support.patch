+  if (scalar.isIntegral(false)) {
+    return scalar.toInt() == 1;
+  } else if (scalar.isFloatingPoint()) {
+    return fabs(scalar.toFloat() - 1.0) < EPSILON;
+  } else {
+    return false;
+  }
+}
