+    OpCommand cmd;
+    cmd.Name("OnesLike").Input(selfContiguous).Output(selfContiguous).Run();
+    NpuUtils::format_fresh_view(self, selfContiguous);
+  } else {
+    OpCommand cmd;
+    cmd.Name("OnesLike").Input(self).Output(self).Run();
+  }
+
